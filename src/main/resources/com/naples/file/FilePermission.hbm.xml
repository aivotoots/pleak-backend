<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping">
<hibernate-mapping>
    <class name="com.naples.file.FilePermission" table="file_permissions" catalog="pleak">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>

        <many-to-one name="file" class="com.naples.file.File" fetch="select" cascade="save-update">
            <column name="file_id" not-null="true" />
        </many-to-one>

        <many-to-one name="user" class="com.naples.user.User" fetch="select" cascade="save-update">
            <column name="user_id" not-null="true" />
        </many-to-one>

        <many-to-one name="action" class="com.naples.helper.Action" lazy="false" fetch="select" cascade="save-update">
            <column name="action_id" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>
